services:
  postgres:
    image: postgres
    environment:
      POSTGRES_PASSWORD: "${YOUR_POSTGRES_PASSWORD}"
      POSTGRES_USER: "${YOUR_POSTGRES_USER}"
      POSTGRES_DB: "${YOUR_POSTGRES_DB_NAME}"
    ports:
      - "5432:5432"
  pgadmin:
    image: dpage/pgadmin4
    environment:
      PGADMIN_DEFAULT_EMAIL: "${YOUR_EMAIL}"
      PGADMIN_DEFAULT_PASSWORD: "${YOUR_PASSWORD}"
    ports:
      - "15432:80"
